<?xml version="1.0" encoding="UTF-8" ?>
<facelet-taglib version="2.0"
		xmlns="http://java.sun.com/xml/ns/javaee"
		xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facelettaglibrary_2_0.xsd">
	<namespace>http://tikron.de/tikron-common-faces</namespace>
	<tag>
		<tag-name>outputDelimited</tag-name>
		<component>
			<component-type>de.tikron.faces.OutputDelimited</component-type>
			<renderer-type>de.tikron.faces.OutputDelimited</renderer-type>
		</component>
	</tag>
	<tag>
		<description>Ein OutputLabel erweitert um die Möglichkeit, auf ein erforderliches Feld hinzuweisen.</description>
		<tag-name>outputLabel</tag-name>
		<component>
			<component-type>de.tikron.faces.OutputLabel</component-type>
			<renderer-type>de.tikron.faces.OutputLabel</renderer-type>
		</component>
	</tag>
	<tag>
		<description>Gibt Text mit Berücksichtigung von Zeilenvorschubzeichen aus.</description>
		<tag-name>outputTextarea</tag-name>
		<component>
			<component-type>de.tikron.faces.OutputTextarea</component-type>
			<renderer-type>de.tikron.faces.OutputTextarea</renderer-type>
		</component>
	</tag>
</facelet-taglib>
